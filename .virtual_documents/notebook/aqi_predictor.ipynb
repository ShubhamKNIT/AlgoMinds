def column_density(df, columns):
    n_rows = len(columns)
    fig, axes = plt.subplots(nrows=n_rows, ncols=1, figsize=(10, 10))

    for i, col in enumerate(columns):
        sns.kdeplot(data=df, x=col,color='red', fill=True, ax=axes[i])

    plt.tight_layout()
    plt.show()






